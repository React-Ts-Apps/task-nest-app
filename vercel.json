{
    "builds": [
        {
            "src": "task-front-end/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "task-front-end/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/task-front-end/dist/index.html"
        }
    ]
}